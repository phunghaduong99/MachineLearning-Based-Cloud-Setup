app_1_workload_intent_name="smf_workload_intent"
app_2_workload_intent_name="pcf_workload_intent"
app_3_workload_intent_name="upf_workload_intent"

app_1_unprotected_if="smf_unprotected_if"
app_1_emco_if="smf_emco_if"
app_2_unprotected_if="pcf_unprotected_if"
app_2_protected_if="pcf_protected_if"
app_2_emco_if="pcf_emco_if"
app_3_protected_if="upf_protected_if"
app_3_emco_if="upf_emco_if"
deployment_intents_in_group="free5gc_deploy_intents"
